<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_saleorder_document_product_note" inherit_id="sale.report_saleorder_document">
        <xpath expr="//th[@name='th_description']" position="after">
            <th name="th_note" class="text-start">Note</th>
        </xpath>
        <xpath expr="//td[@name='td_name']" position="after">
            <td name="td_note"><span t-field="line.product_note"/></td>
        </xpath>
    </template>
</odoo>