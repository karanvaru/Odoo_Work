    <template id="cue_user_wizard" name="Cue User Wizard">
        <section class="cue_wizard_box pt144 pb144" data-vcss="0001" data-snippet="cue_wizard_box"
                 data-name="Wizard Box" style="background-image: none;">
            <div class="o_snippet_invisible s_popup __not_bind" data-vcss="001">
                <div class="modal fade s_popup_middle" id="calltoaction"
                     style="background-color: var(--black-50) !important;" role="dialog" data-bs-backdrop="static"
                     data-bs-keyboard="false" tabindex="-1" aria-labelledby="calltoaction" aria-hidden="true">
                    <div class="modal-dialog d-flex" style="margin-top:25px;">
                        <div class="modal-content modal_div_padding oe_structure">
                            <div class="s_popup_close js_close_popup o_we_no_overlay o_not_editable"
                                 data-bs-dismiss="modal" aria-label="Close">Ã—
                            </div>
                            <section class="s_text_image cue_contact_modal_form pt32 pb32 o_colored_level"
                                     data-name="Text - Image" data-snippet="s_text_image"
                                     style="background-image: none;">
                                <div class="container contact_details">
                                    <div class="contact_header">Upgrade your Home with Cue Home today</div>
                                    <div class="px-100">
                                        <div class="row">
                                            <div class="col-12 col-lg-4">
                                                <label for="fname">
                                                    Name
                                                    <span>*</span>
                                                </label>
                                            </div>
                                            <div class="col-12 col-lg-8">
                                                <input type="text" id="fname" name="firstname"
                                                       class="o_input"
                                                       placeholder="Enter Your Name" required="required"/>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-12 col-lg-4">
                                                <label for="fname">
                                                    Phone Number
                                                    <span>*</span>
                                                </label>
                                            </div>
                                            <div class="col-12 col-lg-8">
                                                <input type="text" id="phone" name="phone"
                                                         class="o_input"
                                                       placeholder="Enter Your Phone Number" required="required"/>
                                            </div>
                                        </div>
                                        <div class="row user_state">
                                            <div class="col-12 col-lg-4">
                                                <label for="user_state">
                                                    State
                                                    <span>*</span>
                                                </label>
                                            </div>
                                            <div class="col-12 col-lg-8">
                                                <select name="user_state" id="user_state"   class="o_input">
                                                    <option value="0" disabled="" selected="" data-calc="0">Select your
                                                        State
                                                    </option>
                                                    <t t-foreach="request.env['res.country.state'].search([('country_id.code','=','IN')])"
                                                       t-as="record">
                                                        <option t-att-value="record.id" t-att-data-id="record.id">
                                                            <t t-esc="record.name"/>
                                                        </option>
                                                    </t>
                                                </select>
                                            </div>
                                        </div>
                                        <!--                                         <div class="row"> -->
                                        <!--                                             <div class="col col-lg-4"> -->
                                        <!--                                                 <label for="user_city"> -->
                                        <!--                                                     Your City -->
                                        <!--                                                     <span>*</span> -->
                                        <!--                                                 </label> -->
                                        <!--                                             </div> -->
                                        <!--                                             <div class="col col-lg-8"> -->
                                        <!--                                                 <input type="text" id="user_city" name="user_city" -->
                                        <!--                                                        placeholder="Enter Your City" required="required"/> -->
                                        <!--                                             </div> -->
                                        <!--                                         </div> -->
                                        <div class="row">
                                            <div class="col-12 col-lg-4">
                                                <label for="user_city">
                                                    City
                                                    <span>*</span>
                                                </label>
                                            </div>
                                            <div class="col-12 col-lg-8">
                                                <select name="user_city"   class="o_input" id="user_city">
                                                    <!--                                                     <option value="0" disabled="" selected="" data-calc="0">Select your -->
                                                    <!--                                                         City -->
                                                    <!--                                                     </option> -->
                                                    <!--                                                     <t t-foreach="request.env['res.city'].search([])" -->
                                                    <!--                                                        t-as="record"> -->
                                                    <!--                                                         <option t-att-value="record.id" -->
                                                    <!--                                                                 t-att-data-id="record.id"> -->
                                                    <!--                                                             <t t-esc="record.name"/> -->
                                                    <!--                                                         </option> -->
                                                    <!--                                                     </t> -->
                                                </select>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-12 col-lg-4">
                                                <label for="user_status">
                                                    Home Status
                                                    <span>*</span>
                                                </label>
                                            </div>
                                            <div class="col-12 col-lg-8">
                                                <select name="user_status" id="user_status"   class="o_input">
                                                    <option value="0" disabled="" selected="" data-calc="0">Select Home
                                                        Status...
                                                    </option>
                                                    <option value="upgrade_existing_system">Upgrade Existing System
                                                    </option>
                                                    <option value="explore_new_system">Explore New System</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-10 btn_class pt10">
                                                <button type="submit"
                                                        class="submit_wizard_data btn btn-primary rounded-circle s_website_form_send btn_font_yellow">
                                                    Submit
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="container thanks_div">
                                        <h1 class="text-center">Thank You!</h1>
                                        <p>We'll be in touch soon to bring a powerful upgrade to your home.</p>
                                    </div>
                                </div>
                            </section>
                        </div>
                    </div>
                </div>
            </div>
            <div class="container px-100 s_allow_columns">
                <div class="row">
                    <div class="col-8">
                        <h1>
                            <font style="color: rgb(225, 225, 240);">
                                <span class="cue_container" style="font-size: 36px;font-weight:400;">Click To Action
                                </span>
                            </font>
                        </h1>
                    </div>
                    <div class="button_box col-4 text-end">
                        <button type="button" class="btn btn-lg btn-primary rounded-circle" data-bs-toggle="modal"
                                data-bs-target="#calltoaction">Click To Open
                        </button>
                    </div>
                </div>
            </div>
        </section>
    </template>
    <template id="template_user_city_wizards">
        <div class="row">
            <select name="user_city" id="user_city">
                <option value="0" disabled="" selected="" data-calc="0">Select your city</option>
                <t t-foreach="user_cities" t-as="c">
                    <option t-att-value="c.id">
                        <t t-esc="c.name"/>
                    </option>
                </t>
            </select>
        </div>
    </template>


    <record id="cue_website.cue_wizard_000_scss" model="ir.asset">
        <field name="name">Cue Wizard SCSS 000</field>
        <field name="bundle">web.assets_frontend</field>
        <field name="path">cue_website/static/src/snippets/cue_wizard/000.scss</field>
    </record>

