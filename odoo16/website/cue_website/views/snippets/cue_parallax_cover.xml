<?xml version="1.0" encoding="utf-8"?>
<odoo>

<template id="cue_parallax_cover" name="Cover Parallax">
    <section class="s_cover cue_parallax_cover parallax s_parallax_is_fixed bg-black-50 pt96 pb96 o_colored_level" data-scroll-background-ratio="1" data-snippet="s_cover" data-name="Cover" style="background-image: none;">
       <span class="s_parallax_bg oe_img_bg" style="background-image: url('/cue_website/static/src/images/cue_parallax_cover.jpg'); background-position: 50% 0;"></span>
       <div class="o_we_bg_filter cue_parallax_back_shaddow"></div>
       <div class="s_allow_columns container px-100">
       	
          <h1 class="mw-460 snippet_title_font">Peace of Mind</h1>
          <span class="lead mw-460 snippet_content_font" >Navigating the intricacies of planning and establishing a <br/>Smart Home can be overwhelming.</span><br/>
          <span class="lead mw-460 snippet_content_font" >However, our dedicated partners are here to alleviate <br/>your concerns and assist you throughout your entire <br/>Smart Home endeavour, ensuring you receive <br/>unwavering support at every stage.</span>
       </div>
    </section>
</template>

<record id="cue_website.cue_parallax_cover_000_scss" model="ir.asset">
    <field name="name">Cue 000 SCSS</field>
    <field name="bundle">web.assets_frontend</field>
    <field name="path">cue_website/static/src/snippets/cue_parallax_cover/000.scss</field>
</record>

</odoo>