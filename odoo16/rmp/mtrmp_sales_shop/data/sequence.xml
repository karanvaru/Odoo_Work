<odoo>
    <data noupdate="1">
        <record id="sequence_sales_shop" model="ir.sequence">
            <field name="name">Sales Shop</field>
            <field name="code">product.sku.sequence</field>
            <field name="active">TRUE</field>
            <field name="prefix">Sale/Shop</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>


        <record id="sequence_shop_order_log" model="ir.sequence">
            <field name="name">shop.fail.log</field>
            <field name="code">shop.fail.log</field>
            <field name="active">TRUE</field>
            <field name="prefix">SH_LOG</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>
