<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for commission.history -->
        <record id="seq_commission_history" model="ir.sequence">
            <field name="name">Commission History</field>
            <field name="code">commission.history</field>
            <field name="prefix">CH/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_commission_history_lines" model="ir.sequence">
            <field name="name">Commission History Line</field>
            <field name="code">commission.history.line</field>
            <field name="prefix">L</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
