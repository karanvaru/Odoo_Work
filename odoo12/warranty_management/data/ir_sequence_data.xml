<odoo>
    <data noupdate="1">

        <!-- Sequences for warranty.registration -->
        <record id="seq__wk_warranty_registration" model="ir.sequence">
            <field name="name">Warranty Registration</field>
            <field name="code">warranty.registration</field>
            <field name="prefix">Warranty/%(range_year)s/%(month)s/</field>
            <field name="padding">3</field>
            <field name="use_date_range">True</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
