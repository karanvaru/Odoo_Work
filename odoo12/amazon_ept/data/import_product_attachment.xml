<odoo>
	<data noupdate="1">
		<record id="ir_attachment_import_product_template_ept" model="ir.attachment">
		<field name="name">import_product_sample.csv</field>
		<field name="datas_fname">import_product_sample.csv</field>
		<field name="datas" type="base64" file="amazon_ept/data/import_product_sample.csv"/>
		<field name="res_model">amazon.process.import.export</field>
		<field name="public">True</field>
		</record>
		<!--Added by Keyur for inbound_shipment_plan_products-->
        <record id="ir_attachment_import_inbound_shipment_product_ept" model="ir.attachment">
            <field name="name">inbound_shipment_plan_sample.csv</field>
            <field name="datas_fname">inbound_shipment_plan_sample.csv</field>
            <field name="datas" type="base64" file="amazon_ept/data/inbound_shipment_plan_sample.csv"/>
            <field name="res_model">import.product.inbound.shipment</field>
            <field name="public">True</field>
        </record>
        <record id="ir_attachment_amazon_inbound_shipment_box_content" model="ir.attachment">
            <field name="name">amazon_inbound_shipment_box_content.csv</field>
            <field name="datas_fname">amazon_inbound_shipment_box_content.csv</field>
            <field name="datas" type="base64" file="amazon_ept/data/amazon_inbound_shipment_box_content.csv"/>
            <field name="res_model">amazon.inbound.shipment.wizard</field>
            <field name="public">True</field>
        </record>
	 </data>
 </odoo>
