<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <!-- Repair Order Sequence-->
    <record id="repair.seq_repair" model="ir.sequence">
        <field name="name">Repair Order</field>
        <field name="code">repair.order</field>
        <field name="prefix">REPAIR</field>
        <field name="padding">5</field>
    </record>

    <!-- RMA Sequence-->
    <record id="seq_rma_issue" model="ir.sequence">
        <field name="name">RMA Issue</field>
        <field name="code">rma.issue</field>
        <field name="prefix">RMA</field>
        <field name="padding">5</field>
    </record>

</odoo>