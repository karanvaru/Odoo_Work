<?xml version="1.0" encoding="UTF-8"?>

<templates id="template" xml:space="preserve">
    <t t-name="pos_extension.Navbar" t-inherit="point_of_sale.Navbar" t-inherit-mode="extension">
        <xpath expr="//li[contains(@t-on-click, 'pos.toggleImages')]" position="attributes">
                <attribute name="style">display: none</attribute>
        </xpath>
        <xpath expr="//li[contains(@t-on-click, 'pos.toggleImages') and contains(., 'Hide Category Images')]"
               position="attributes">
               <attribute name="style">display: none</attribute>
        </xpath>
        <xpath expr="//li[contains(@t-on-click, 'onTicketButtonClick')]"
               position="attributes">
               <attribute name="style">display: none</attribute>
        </xpath>

    </t>
</templates>


